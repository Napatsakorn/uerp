{% extends 'UerpMainBundle::main.html.twig' %}
{% block title %} Product ICMS {% endblock %}

{% block body -%}
    <div class="panel panel-info" style="margin: 20px">
    <div class="panel-heading"> 
    <h3>ProductIcms list</h3>
	</div>
 	 <div class="panel-body">
	    <table class="table table-striped table-advance table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>CSON</th>
                <th>Description</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('product_icms_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.cson }}</td>
                <td>{{ entity.description }}</td>
                <td>

                        <a class="btn btn-info btn-xs"  href="{{ path('product_icms_show', { 'id': entity.id }) }}">show</a>
                        <a class="btn btn-warning btn-xs" href="{{ path('product_icms_edit', { 'id': entity.id }) }}">edit</a>

                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a class="btn btn-success" href="{{ path('product_icms_new') }}"> Create a new entry</a>
        </li>
    </ul>
    {% endblock %}
