	<script type="application/javascript">
        $(document).ready(function () {



myvar = setInterval(function () {delay();}, 3000);


function delay (){ 

     $('#sidebar > ul').hide();
      $('#main-content').css({
                'margin-left': '0px'
            });
            $('#sidebar').css({
                'margin-left': '-210px'
            });
